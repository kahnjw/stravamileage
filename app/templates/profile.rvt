<div class="profile-page">
  <img rv-src="profile.profile" class="picture">
  <div class="basic-info">
    <h2>{profile.firstname} {profile.lastname}</h2><span>{profile.city}, {profile.state}</span>
  </div>
  <div class="controls">
    <a href="#logout" class="logout">
      <i class="fa fa-sign-out"></i>
      Logout
    </a>
  </div>
  <div class="clearboth"></div>
  <div class="activities">
  </div>
</div>
